<tool id="CONVERTER_robo_to_fasta" name="Convert robo to fasta" version="1.0.0">
    <command>python '$__tool_directory__/robo_converter.py' '$input' '$output' 'f'</command>
    <inputs>
        <param name="input" type="data" format="robo" label="robo file"/>
    </inputs>
    <outputs>
        <data name="output" format="fasta"/>
    </outputs>
    <help>
    </help>
</tool>
